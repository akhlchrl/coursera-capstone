    "postal_codes = postal_codes.groupby(['PostalCode', 'Borough'])['Neighborhood'].apply(list)\n",
    "\n",
    "postal_codes = postal_codes.sample(frac=1).reset_index()\n",
    "postal_codes['Neighborhood'] = postal_codes['Neighborhood'].str.join(',')\n",
    "postal_codes.head()"
